@model List<Manage_Receive_Issues_Goods.Models.Supplier>

@{
    ViewData["Title"] = "Supplier List";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"]</title>

    <style>

        body {
            font-family: "Lato", sans-serif;
            background-image: url('/images/white.jpg');
            background-size: cover;
            background-repeat: no-repeat;
            background-attachment: fixed;
            background-position: center;
             }

    </style>


</head>
<body>
    <div style="display: flex; gap: 10px;">
        <a class="nav-link btn btn-dark" asp-area="" asp-controller="TLIPWarehouse" asp-action="ScheduleReceive">Received</a>
        <a class="nav-link btn btn-dark" asp-area="" asp-controller="TLIPWarehouse" asp-action="ScheduleIssued">Issued</a>
        <a class="nav-link btn btn-dark" asp-area="" asp-controller="TLIPWarehouse" asp-action="SupplierList">Supplier List</a>
    </div>
    <table class="table">
        <thead>
            <tr>
                <th>Supplier Code</th>
                <th>Supplier Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var supplier in Model)
            {
                <tr>
                    <td>@supplier.SupplierCode</td>
                    <td>@supplier.SupplierName</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>



